@model List<Fortune>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Fortunes</title>
</head>
<body>

<table>
<tr>
    <th>id</th>
    <th>message</th>
</tr>
@foreach (var fortune in Model)
{
<tr>
    <td>@fortune.id</td>
    <td>@fortune.message</td>
</tr>
}
</table>

</body>
</html>